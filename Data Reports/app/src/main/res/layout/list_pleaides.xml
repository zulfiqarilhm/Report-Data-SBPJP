<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:layout_width="73dp"
        android:layout_height="73dp"
        android:src="@drawable/pleaides2"
        android:paddingLeft="15dp"
        android:paddingTop="20dp"
        android:id="@+id/logojpss"/>

    <TextView
        android:id="@+id/DatasetNamePleaides"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/logojpss"
        android:text="{DatasetNamePleaides}"
        android:textColor="#000000"
        android:textSize="17sp"
        android:textStyle="bold"
        android:layout_marginLeft="15dp"
        android:paddingTop="15dp"/>

    <TextView
        android:id="@+id/HasilPleaides"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/logojpss"
        android:text="{Hasil}"
        android:textColor="#F81212"
        android:textSize="17sp"
        android:textStyle="bold"
        android:layout_marginLeft="15dp"
        android:paddingTop="63dp"/>

    <TextView
        android:id="@+id/aa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/logojpss"
        android:text=""
        android:textSize="19sp"
        android:textStyle="bold"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="90dp"
        />





</RelativeLayout>